{
  "manifest_version": 3,
  "name": "ENAIRE Drones Extension",
  "version": "1.2",
  "author": "Sergio Garcia (soukron@gmbros.net)",
  "description": "Extensi√≥n NO OFICIAL para simplificar la lectura en drones.enaire.es.",
  "homepage_url": "https://github.com/soukron/enaire-drones-extension",
  "permissions": [],
  "host_permissions": [
    "https://api.open-elevation.com/*",
    "https://drones.enaire.es/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://drones.enaire.es/*"],
      "js": ["content.js"],
      "type": "module"
    }
  ],
  "web_accessible_resources": [{
    "resources": [
      "modules/*.js",
      "modules/ui/*.js",
      "modules/utils/*.js",
      "aip.json"
    ],
    "matches": ["https://drones.enaire.es/*"]
  }],
  "action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    }
  }
}